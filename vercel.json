{
  "buildCommand": "cd nuxt-app && npm install && npm run build",
  "outputDirectory": "nuxt-app/.output",
  "installCommand": "cd nuxt-app && npm install",
  "framework": null,
  "headers": [
    {
      "source": "/_nuxt/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
